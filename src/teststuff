function CarDetail({ car, onClose }) {
    return (
        <div className="car-details">
            <h2>Car Details</h2>
            <p>Name: {car.name}</p>
            <p>Type: {car.type}</p>
            <p>About: {car.about}</p>

            {/* Knop om de details van een auto te sluiten */}
            <button onClick={onClose}>Close</button>
        </div>
    );
}

export default CarDetail;
